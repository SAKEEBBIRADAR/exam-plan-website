{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/policy",
        "destination": "/policy.html"
      },
      {
        "source": "/contact",
        "destination": "/contact.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
